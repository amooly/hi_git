{
  "name": "hi_git",
  "displayName": "Hi Git",
  "description": "A sample VS Code extension.",
  "version": "0.0.1",
  "publisher": "Harvey",
  "engines": {
    "vscode": "^1.60.0"
  },
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./out/compile.js"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/vscode": "^1.103.0",
    "ts-loader": "^8.0.0",
    "typescript": "^4.9.5",
    "vscode": "^1.1.36",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  }
}
